{
  "framework": "vite",
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/public",
  "installCommand": "npm install",
  "devCommand": "cd client && npm run dev",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://web-production-bb19.up.railway.app/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://web-production-bb19.up.railway.app"
  }
}