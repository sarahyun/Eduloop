{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "devCommand": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://web-production-bb19.up.railway.app/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://web-production-bb19.up.railway.app"
  }
}